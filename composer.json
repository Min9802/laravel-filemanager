{
    "name": "min98/laravel-filemanager",
    "description": "File manager for Laravel",
    "keywords": [
        "laravel",
        "file",
        "manager"
    ],
    "authors": [{
        "name": "Min",
        "email": "hotro.minservices@gmail.com",
        "role": "Developer"
    }],
    "homepage": "https://github.com/min9802/laravel-filemanager",
    "license": "MIT",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^8.1",
        "ext-zip": "*",
        "ext-json": "*",
        "ext-fileinfo": "*",
        "ext-exif": "*",
        "laravel/framework": "^9.0|^10.0|^11.0",
        "league/flysystem": "^3.0",
        "guzzlehttp/guzzle": "7.8.1",
        "intervention/image-laravel": "^1.2.0"
    },
    "autoload": {
        "psr-4": {
            "Min\\FileManager\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Min\\FileManager\\FileManagerServiceProvider"
            ]
        }
    }
}